<h1>TasksController#index</h1>
<p>Find me in app/views/tasks_controller/index.html.erb</p>

<% @tasks.each do |task| %>
<h2><%= task.description %></h2>
<p><%= task.due_date %></p>
<p><%= task.responsible %></p>
<%= link_to "Edit Task", edit_task_path(task) %>
<%= link_to "Delete", task_path(task), method: :delete, data: { confirm: "Are you sure?" } %>
<% end %>
